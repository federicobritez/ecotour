<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Habitaciones" table="habitaciones">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cantCamasSimples" type="integer" column="cant_camas_simples" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="cantCamasDoble" type="integer" column="cant_camas_doble" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="valorHabitacion" type="integer" column="valor_habitacion" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="estadoHabitacion" type="integer" column="estado_habitacion" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
